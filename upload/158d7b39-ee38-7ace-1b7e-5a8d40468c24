<script>

window.__FILES_MAP__ = (null);


window.BROKEN_IMGSMAIL = false;


window.includeSourceRetryError = function (type) {
	require('mrg-xray')('retry_' + type + '_source&v=1&i=error:1');
};

window.includeSourceRetryLoad = function (type) {
	require('mrg-xray')('retry_' + type + '_source&v=1&i=load:1');
};

window.includeCSSError = function (url, attrs) {
	BROKEN_IMGSMAIL = true;

	var documentElement = document.documentElement.firstChild,
		newUrl = url.replace(/.*?\/\/.*?\//, '/'),
		node = document.createElement("link");

	node.href = newUrl;
	node.type = "text/css";
	node.rel = "stylesheet";
	if (attrs) {
		if (attrs.id) {
			node.id = attrs.id;
		}
		if (attrs.media) {
			node.media = attrs.media;
		}
	}
	node.setAttribute("async", "true");
	node.setAttribute("defer", "true");
	node.onload = function () {
		includeSourceRetryLoad("css");
	};
	node.onerror = function () {
		includeSourceRetryError("css");
	};
	documentElement.insertBefore(node, documentElement.firstChild);
};

window.includeJSFilename = function (url) {
	
	BROKEN_IMGSMAIL && (url = url.replace(/.*?\/\/.*?\//, '/'));
	return url;
};






(function (win) {
	var nanoXhr = require('mrg-nano-xhr');
	var filesQueue = [];
	var deferredCode = [];
	var deferredRequire = [];
	var xhrLogger = win.__xhrLogger__ = [];
	var asyncBootReady = false;

	function processing() {
		var task = filesQueue[0];

		if (task && task.complete && task.onload(task.err, task.xhr)) {
			task.xhr = null;
			filesQueue.shift();

			asyncBootReady = true;
			__asyncBoot();

			processing();
		}
	}

	win.__asyncBoot = function (callback, priority) {
		callback && deferredCode[priority ? 'unshift' : 'push'](callback);

		if (asyncBootReady && filesQueue.length === 0) {
			while (deferredCode.length) {
				if (deferredCode[0](win) === false) return;
				deferredCode.shift();
			}
		}
	};

	
		function tryDetectRawError(url, source, id) {
			try {
				var iframe = document.createElement('iframe');

				iframe.style.display = 'none';
				document.body.appendChild(iframe);

				var idoc = iframe.contentWindow && iframe.contentWindow.document || iframe.contentDocument;
				idoc.open();
				idoc.write([
					'<script>window.onerror=function (m, u, l, c, e) {',
						'(' + require('mrg-xray').toString() + ')("head__xhr_eval_errs&v=1&rlog=xhr_js_eval_errs&rlog_message=" + encodeURIComponent(JSON.stringify({',
							'id: "' + id + '",',
							'url: "' + url + '",',
							'msg: m,',
							'line: l,',
							'column: c,',
							'name: e && e.name,',
							'stack: e && e.stack',
						'})));',
					'};</' + 'script>',
					'<script>' + source + '</'+ 'script>'
				].join('\n'));
				idoc.close();
			} catch (_) {}
		}
	

	win.includeJS = function (url, catchCallbacks, attempts, task) {
		url = includeJSFilename(url);

		attempts = attempts | 0;

		var onLoad = function (err, _xhr) {
			var id = (new Date().valueOf() + Math.random()).toString(36),
				details = [],
				filename = _xhr.url.split('/').pop().replace(/\.js(\?.*)?$/, '').replace(/\./g, '-'),
				evalTime = new Date(),
				duration = _xhr.duration,
				source = String(_xhr.responseText),
				sourceSize = source.length,
				detailsQuery = [],
				radarQuery = [],
				evalError;

			details['try'] = details['try_' + filename] = attempts;
			details[_xhr.status] = details[filename] = details[filename + '_' + _xhr.status] = duration;

			try {
				details['size_' + filename] = sourceSize;

				if (err || !sourceSize) {
					details['error'] = 1;
					details['err_' + filename] = 1;
				} else {
					window.eval.call(window, source + '//# sourceURL=' + _xhr.url);
					duration += (details['eval'] = details['eval_' + filename] = new Date() - evalTime);
				}
			} catch (error) {
				err = true;
				evalError = error;
				details['error'] = details['eval_error'] = details['eval_err_' + filename] = 1;

				
					tryDetectRawError(_xhr.url, source, id);
				
			}

			if (!sourceSize) {
				details['empty_' + filename] = 1
			}

			for (var key in details) {
				detailsQuery.push(key + ':' + details[key]);
			}

			radarQuery.push('v=' + Math.max(duration, 1), 'i=' + detailsQuery.join(','), 'rnd=' + id);

			if (err || !sourceSize) {
				radarQuery.push(
					'rlog=xhr_js_loader_' + _xhr.status + (sourceSize > 0 ? '' : '_empty'),
					'rlog_message=' + encodeURIComponent(JSON.stringify({
						id: id,
						attempts: attempts,
						url: _xhr.url,
						name: filename,
						size: sourceSize,
						lines: source.split(/\n/).length,
						duration: duration,
						headers: (_xhr.getAllResponseHeaders() || '').split(/\r?\n/),
						readyState: _xhr.readyState,
						status: _xhr.status,
						statusText: _xhr.statusText,
						location: location.toString(),
						xhrError: _xhr.err + '',
						evalError: evalError + '',
						allErrors: __errors__,
						externalErrors: __externalErrors__
					}))
				);
			}

			require('mrg-xray')('head__xhr_' + attempts + '&' + radarQuery.join('&'));

			if (sourceSize < '10240') {
				var offset = 0;
				var partSize = 1024 * 2;

				do {
					require('mrg-xray')('head__xhr_err-parts&rlog=xhr_js_errs&rlog_message=' + encodeURIComponent(JSON.stringify({
						id: id,
						name: filename,
						offset: offset,
						partSize: partSize,
						sourceSize: sourceSize,
						chunk: source.substr(offset, partSize)
					})));
					offset += partSize;
				} while (offset < sourceSize);
			}

			xhrLogger.push({
				url: _xhr.url,
				error: _xhr.err,
				evalError: evalError,
				details: details,
				duration: duration,
			});

			if (err && (attempts < "1")) {
				console.warn('xhr.load:', url, err);

				
				BROKEN_IMGSMAIL = true;
				
				includeJS(url, catchCallbacks, attempts + 1, task);

				return false;
			}

			return true;
		};

		if (!task) {
			task = {};
			filesQueue.push(task);
		}
		task.onload = onLoad;

		
			var sync = true;
		

		nanoXhr.request({url: url, sync: sync}, function xhrLoaderCompleted(error, _xhr) {
			task.xhr = _xhr;
			task.err = error;
			task.complete = true;

			processing();
		});
	};
})(window);



