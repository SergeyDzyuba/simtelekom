	(function (patron, build) {
		function pad(number) {
			return ((number < 10) ? '0' : '') + number;
		}

		function toISOString(date) {
			if (date.toISOString) {
				return date.toISOString();
			}

			return (date.getUTCFullYear() +
				'-' + pad(date.getUTCMonth() + 1) +
				'-' + pad(date.getUTCDate()) +
				'T' + pad(date.getUTCHours()) +
				':' + pad(date.getUTCMinutes()) +
				':' + pad(date.getUTCSeconds()) +
				'.' + (date.getUTCMilliseconds() / 1000).toFixed(3).slice(2, 5) +
				'Z');
		}

		var matches = /^[^-]+-f-(.+)-([^-]+)-(\d+)\.tgz$/.exec(build);

		if (matches) {
			patron.buildInfo = {
				tarball: build,
				branch: matches && matches[1],
				username: matches && matches[2],
				date: matches && new Date(matches[3] * 1000)

				
					, packages: {"ajs":"0.18.4","callstackjs":"1.0.0","clipboard":"1.5.6-rc.8","compose":"0.5.2","features":"0.1.2","FileAPI":"0.2.6","file-icon":"0.1.0","html5-history-api":"4.2.0","jquery-packir":"0.7.10","jssdk":"0.48.0","jukebox":"0.0.7","mailru.portal-menu.desktop":"0.2.0","modernizr":"1.0.4","mrg-context":"1.10.6","mrg-api-encryptor":"1.1.0","mrg-honeypot":"1.3.0","mrg-smokescreen":"1.9.1","suggests":"0.1.11","mrg-explorer":"0.4.3","mrg-form":"0.4.7","mrg-i18n-utils":"0.6.2","mrg-message-broker":"1.3.0","mrg-polyfills":"0.3.2","mrg-recaptcha":"0.2.1","news-widget":"0.4.13","beta-user-widget":"0.1.2","packir":"0.7.10","push-manager":"1.0.14","require-fest":"0.1.0","requirejs-injector":"0.1.2","requirejs-lite":"0.7.8","mrg-nano-xhr":"0.0.15","mrg-load-log":"0.1.1","mrg-xray":"0.2.0","mrg-text-analyzer":"0.2.1","tinymce":"0.1.55","toolkit":"1.19.3","toolkit-common":"0.3.1","translator":"0.0.6","viewer":"0.3.10","turntable":"0.0.4","mrg-folder-utils":"0.0.5","uuid":"0.1.0","mrg-js-cookie":"0.0.3"}
				
			};

			patron.build = patron.buildInfo.branch + '.' + toISOString(patron.buildInfo.date).split('.')[0].replace(/:/g, '_');

		} else {
			patron.buildInfo = null;
			patron.build = build;
		}
	})(window.patron, 'e.mail.ru-f-bravo-mail-63326-a.ostapenko-1518535690.tgz');

	
