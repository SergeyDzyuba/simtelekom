	<script type="text/javascript">
		patron.DIRECT_LEFT_COUNT = "3"|0;

		require([
			'advert/Ad',
			'advert/mimic/Locator'
		],
		function (Ad, locator) {
			function _getCssString(elem) {
				var el = $(elem), color = el.css('content') || el.css('fontFamily');
				// for IE8+
				if (color === 'normal') color = el.css('fontFamily');
				return color.replace(/["']/g,'');
			}

			function getDirectTheme(){
				var colors = {};
				$('#direct-colors i').each(function() {
					colors[this.className.replace('direct-colors__', '')] = _getCssString(this);
				});
				return colors;
			}

			patron.DirectTheme = getDirectTheme();

			Ad.push(
				{	// 1514
					  id:			'direct-left'
					, placeId:		locator.transform('rb-direct-left-slot')
					, reloadable:	false
					, disabled:		true
					, ads:			[{ order: 10, partner_id: 27219, stat_id: '123'.replace(/[^\d]/g, ''), ad_format: "direct", type: "mailruVertical", limit: 3, title_font_size: 1, font_size: 1, border_color: patron.DirectTheme.borderColor, site_bg_color: patron.DirectTheme.bg, title_color: patron.DirectTheme.leftLink, hover_color: patron.DirectTheme.leftHover, url_color: patron.DirectTheme.leftInfo, text_color: patron.DirectTheme.leftText, favicon: true, no_sitelinks: true }]
				},

			

			
				{	// 1545
					  id:			'sendmsgok-bottom'
					, placeId:		locator.transform('preload_banner_1')
					, reloadable:	false
					, disabled:		true
					, ads:			[{ order: 400, partner_id: 27219, stat_id: '987666'.replace(/[^\d]/g, ''), ad_format: 'direct', title_font_size: 1, font_size: 1, type: 'horizontal', limit: 3, site_bg_color: patron.DirectTheme.bgSentMsgOk, title_color: patron.DirectTheme.leftLink, hover_color: patron.DirectTheme.leftHover, url_color: patron.DirectTheme.leftInfo, text_color: patron.DirectTheme.leftText, favicon: true, no_sitelinks: true }]
				},
			

			
				{	// 1480
					  id:			'advertising-topline'
					, placeId:		locator.transform('YaDirectLine')
					, reloadable:	false
					, disabled:		true
					, ads:			[{ order: 1000, partner_id: 27219, stat_id: '654384386'.replace(/[^\d]/g, ''), type: 'row', ad_format: "direct", title_font_size: 2, font_size: 1, limit: 1, title_color: patron.DirectTheme.top, url_color: patron.DirectTheme.top, all_color: patron.DirectTheme.top, text_color: patron.DirectTheme.top, hover_color: patron.DirectTheme.topHover, favicon: true, disable_highlight: true }]
				}
			
			);
		});
	</script>






