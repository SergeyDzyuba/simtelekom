patron['themes'] = (function() {
	function data(theme, source) {
		var res = { };

		try {
			if (!source.length) {
				throw new Error('Empty source')

			} else {
				res = JSON.parse(source);
			}

		} catch (e) {
			console.error(e);
			patron.radarLog('bad-theme-widget', { theme: theme }, 1);
		}

		return res;
	}

