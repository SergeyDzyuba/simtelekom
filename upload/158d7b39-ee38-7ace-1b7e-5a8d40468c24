	function _strToList(str) {
		return str
				.replace(/(^\s+|\s+$)/g, '')
				.split(/\s+/)
				.map(function(item) {
					var split = item.split(':'),
						id = split[0];

					if (split.length > 1) {
						var theme = themes[id];

						if(theme) {
							theme.types = split[1].split(',')
						}
					}

					return id;
				});
	}

	var settingsListIds = "default t4001 t4002 t4003 t4004 t4005 t4006 t4007 t4008 t4009 t4010 t4011 t4012 t4013 t4014 t4015 t4000 t1101 t3014:new,big t1189 t3012 t3011 t3005 t3007 t3008 t3009 t1190 t3013 t1001 t1087:popular t3000 t1002 t1078 t1071 t1069 t1081 t1075 t2012 t2013 t2014 t2015 t2016 t2017 t2019 t2020 t2022 t2023 t2024:big t2025 t2026 t2028 t2031 t2032 t2033 t1151 t1156 t1119 t1121 t1112 t1108:popular t1015:popular,big t1086:popular,dynamics t3002 t1025 t1114 t1115 t1116 t1122 t1123 t1120 t1117 t1118 t1105:temporary t1106:temporary t1132 t1079 t1046 t1047 t1088:popular t1056:popular,dynamics t1113:dynamics t1109 t1051:popular,temporary t1089 t1149 t1150 t1070 t1064 t1065 t1058:contrast t1057 t1000:popular t1007:popular t1003:popular t1004 t1016:dynamics t1006 t1022 t1009 t1011 t1010 t1038 t1027 t1052 t1008 t1055:popular t1054:popular t1042 t1083 t1077 t1018 t1050 t1053:contrast t1104:contrast t1060:dynamics t1045:contrast t1017 t1080 t1024 t1013 t1040:contrast t1005:contrast,popular t2027 t1031 /  t1063:contrast t1191 t1192 t1193:contrast t1194:contrast t1195 t1196 t1197 t1199:contrast t1198 t1136 t1135 t1137:big t1144 t1147 t1148 t1145 t1128 t1131 t1126 t1139:big t1129 t1141 t1142 t1138 t1130 t1146 t1133 t1134 t1140 t1125 t1124 t1153 t1090:popular,dynamics /  t2018 t2011 t2034 t1157:new,big t1073 t1072 t1076 t1028 t1032 t1034".split(/\s*\/\s*/).map(function(listStr) {
		return {
			list: _strToList(listStr)
		};
	});


	var tutorialIds = _strToList(" default t3002 t1119 t1015 t2024 t2017 t2025 t2018 t2013 t2034 t1086 t1115 t2023 t1190 t1075 t1071 t1116 t2015 t2020 t1120 t2011 t1069 t1117 t1118 t1078");
	var tutorialAdaptiveBgIds = _strToList(" default t1119 t1015 t2024 t2017 t2025 t2018 t2013 t2034 t1086 t1115 t2023 t1190 t1075 t1071 t1116 t2015 t2020 t1120 t2011 t1069 t1117 t1118 t1078");

	 function _getTheme(id) {
		var data = themes[id];
		if(data) {
				data.id = id;
			}

		return data;

	 }

	 function _getPictruredTheme(id) {
		var data = themes[id];

		if (data && !data.colorized) {
			if (data) {
				data.id = id;
			}

			return data;
		} else {
			return false;
		}

	 }

	 function _getColorizedTheme(id) {
		var data = themes[id];

		if (data && data.colorized) {
			if (data) {
				data.id = id;
			}

			return data;
		} else {
			return false;
		}

	 }

	return {
	 	list: themes,

		getSettingsLists: function() {
			return Array.map(settingsListIds, function (obj) {
				return Array.filter(obj.list, _getPictruredTheme).map(_getTheme);
			});
		},

		getSettingsColorizedLists: function() {
			return Array.map(settingsListIds, function (obj) {
				return Array.filter(obj.list, _getColorizedTheme).map(_getColorizedTheme);
			});
		},

		getInitialList: function() {
			return Array.map(tutorialIds, _getTheme);
		},

		getInitialAdaptiveBgList: function() {
			return Array.map(tutorialAdaptiveBgIds, _getTheme);
		},

		get: _getTheme
	}
})();

require('mrg-load-log').end('lang_themes');
</script>


