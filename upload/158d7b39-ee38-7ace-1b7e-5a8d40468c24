window.includeCSS = function (url, attrs) {
	if(!window.includeCSS.__list) {
		window.includeCSS.__list = [];
	}
	window.includeCSS.__list.push(url);

	if(BROKEN_IMGSMAIL) {
		url = url.replace(/.*?\/\/.*?\//, '/');
	}

	attrs = attrs || {};
	attrs.id = attrs.id || '';
	attrs.media = attrs.media || '';

	var attrStr = (attrs.id ? ' id="' + attrs.id + '"' : '') + (attrs.media ? ' media="' + attrs.media + '"' : '');

	
		document.writeln('<link href="' + url + '" rel="stylesheet" type="text/css" ' + attrStr + ' onerror="includeCSSError(\'' + url + '\', { id: \'' + attrs.id + '\', media: \'' + attrs.media + '\' });" />');
	
};

