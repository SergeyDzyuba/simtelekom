<script>
	window.__errors__ = [];
	window.__externalErrors__ = [];

	window.onerror = function globalErrorHandler(msg, url, line, column, error) {
		var record = {
			message: msg,
			url: url,
			line: line,
			column: column,
			error: error,
		};

		if (url && !/((^(chrome|file):)|(miscellaneous|extension)_bindings)/.test(url)) {
			__errors__.push(record);
			(window.logger != null) && logger.add('global.error', record);
		} else {
			__externalErrors__.push(record);
		}

		require('mrg-load-log').error(record);

		return true;
	};

	window.onerror.add = function globalErrorListener(callback) {
		var prev = window.onerror;

		window.onerror = function anotherGlobalErrorHandler() {
			prev.apply(this, arguments);
			callback.apply(this, arguments);
			return true;
		};
	};

	window.__handledByObserver__ = [];

	try {
		if (Object && typeof Object.defineProperty === 'function' && typeof Proxy !== 'undefined') {
			(function () {
				function createProxy(target) {
					return new Proxy(target, {
						set: function (target, prop, value) {
							var err = new Error();
							window.__handledByObserver__.push(['set', Date.now(), prop, value, err.stack]);
							target[prop] = value;
							return true;
						},
						deleteProperty: function (target, phrase) {
							var err = new Error();
							window.__handledByObserver__.push(['delete', Date.now(), phrase, err.stack]);
							delete target[phrase];
							return true;
						}
					});
				}

				var patronProxy = createProxy(window.patron);
				window.__handledByObserver__.push(['setup', Date.now()]);
				Object.defineProperty(window, 'patron', {
					'configurable': true,
					'set': function (value) {
						var err = new Error();
						window.__handledByObserver__.push(['rewrite patron', Date.now(), value, err.stack]);
						if (value && typeof value === 'object') {
							patronProxy = createProxy(value);
						} else {
							patronProxy = value;
						}
					},
					'get': function () {
						return patronProxy;
					}
				});
			})();
		}
	} catch (err) {
		__handledByObserver__.push(err);
	}

	require('mrg-load-log').end('head__onerror');
</script>


