<script>
(function() {
	try {
		var document = window.document;
		var state = 'init';
		var substate = 'init';
		var sendState = '';

		var loadLog = window.require && require.defined && require.defined['mrg-load-log'] ? require('mrg-load-log') : undefined;

		var from =
				'msglist';
		
		;

		var getData = function(result, data) {
			data.result = result;
			data.from = from;
			data.state = state;
			data.substate = substate;

			try {
				data.readyState = document.readyState;
			} catch (e) {
				data.readyState = e;
			}

			try {
				data.time = (new Date).valueOf() - document.TS.valueOf()
			} catch (e) {
				data.time = e;
			}

			try {
				data.timing = (performance.timing.loadEventEnd || (new Date).valueOf()) - performance.timing.navigationStart;
			} catch (e) {
				data.timing = e;
			}

			data.__errors__ = window.__errors__;
			data.__externalErrors__ = window.__externalErrors__;

			if (window.__handledByObserver__) {
				data.__handledByObserver__ = window.__handledByObserver__.length;
			}

			try {
				data.patron = window.patron ? Object.keys(window.patron).length : String(window.patron);
			} catch (e) {
				data.patron = e;
			}

			try {
				data.ph = window.__PH ? Object.keys(window.__PH).length : String(window.__PH);
			} catch (e) {
				data.ph = e;
			}

			data.threads = false;

			try {
				if (loadLog) {
					function convert(items) {
						var result = {};

						for (var i in items) {
							result[i] = {
								s: items[i].state,
								t: (items[i][items[i].state].ts.valueOf() - first.row.start.ts)
							};
						}

						return result;
					}
					var logItems = loadLog.result().items;
					var first = loadLog.result().events[0];

					if (loadLog.ns) {
						data.loadLog = {};
						for (var i in logItems) {
							data.loadLog[i] = convert(logItems[i]);
						}
						
					} else {
						data.loadLog = convert(logItems);
					}
				}
			} catch (e) {
				data.loadLog = e;
			}

			try {
				data.location = window.location.toString();
			} catch (e) {
				data.location = e;
			}

			try {
				data.features = patron.FEATURES ? patron.FEATURES.split(/\s+/).length : String(patron.FEATURES);
			} catch (e) {
				data.features = e;
			}

			try {
				data.honeypot = patron.honeypot.length ? patron.honeypot.length : String(patron.honeypot);
			} catch (e) {
				data.honeypot = e;
			}

			return data;
		};

		var radar = function(label, sendData, data, save) {
			if (/_after-ok/.test(sendState)) {
				sendData = false;
			}

			var _data = getData(label + '=1', data || {});

			var url = [
				'//stat.radar.imgsmail.ru/update?email=sim-sim.com@mail.ru',
				'p=mail',
				't=broken_' + from,
				'v=1',
				'i=' + label + sendState + ':1',
				'rnd=' + Math.random()
			];

			if (save && save.params && save.params.length) {
				for (var pos = 0; pos <= save.params.length; pos++) {
					var name = String(save.params[pos]);

					if (name in _data) {
						url.push(name + '=' + _data[name]);
					}
				}
			}

			if (sendData) {
				url.push('rlog=broken');
				url.push('rlog_message=' + encodeURIComponent(JSON.stringify(_data)));
			}

			(new Image).src = url.join('&');
			sendState += '_after-' + label;
		};

		function error(e) {
			radar('error', true, {
				err: (e || 'none').toString().substr(0, 1024)
			}, 1);
		}

		try {
			var waitTimeout, timeoutTimeout;

			function checkDom() {
				state = 'check-dom';

				try {
					var ROOT_NODE, CHILD_CNT;

					switch (from) {
						case 'attachviewer':
							ROOT_NODE = 'AttachmentViewer';
							CHILD_CNT = 1;
							break;

						case 'signup':
							ROOT_NODE = 'LEGO';
							CHILD_CNT = 2;
							break;

						default:
							ROOT_NODE = 'LEGO';
							CHILD_CNT = 3;
							break;
					}

					var lego = document && document.getElementById ? document.getElementById(ROOT_NODE) : window[ROOT_NODE];

					if (!lego) {
						radar('no-lego', true);

					} else if (lego.getElementsByClassName('error-message').length > 0) {
						var code = 'unknown';

						try {
							code = lego.getElementsByClassName('error-message')[0].getElementsByTagName('img')[0].getAttribute('alt');
						} catch (e) {}

						radar('message-' + code, true);

					} else if (!lego.children || lego.children.length < CHILD_CNT) {
						if (!lego.children) {
							substate = 'no-children';

						} else {
							substate = 'children-' + lego.children.length;
						}

						radar('no-childs', true);
					} else {
						radar('ok', false, {}, {params: ['time', 'html']});
						clearListeners();
					}
				} catch (e) {
					error(e);
				}
			}

			function wait(callback) {
				if (waitTimeout) {
					window.clearTimeout(waitTimeout);
				}

				waitTimeout = window.setTimeout(callback, 100);
			}

			function checkApp() {
				state = 'check-app';

				try {
					var p = window.patron;

					switch (from) {
						case 'attachviewer':
							if (p.AttachmentViewer && p.AttachmentViewer.activeRequest && p.AttachmentViewer.activeRequest === p.AttachmentViewer.request) {
								checkDom();
							} else {
								if (!p.AttachmentViewer) {
									substate = 'no-viewer'

								} else if (!p.AttachmentViewer.activeRequest) {
									substate = 'no-active-request'

								} else if (p.AttachmentViewer.activeRequest !== p.AttachmentViewer.request) {
									substate = 'different-request'
								} else {
									substate = 'unknown';
								}

								wait(checkApp);
							}
							break;

						case 'addressbook':
							if (p.addressbook) {
								substate = 'wait-dom';
								wait(checkDom);

							} else {
								if (!p.addressbook) {
									substate = 'no-addressbook'
								} else {
									substate = 'unknown';
								}
								wait(checkApp);
							}
							break;

						case 'msglist':
							if (p.Router && p.Router.activeRequest && p.Router.activeRequest === p.Router.request) {
								substate = 'wait-dom';
								wait(checkDom);

							} else {
								if (!p.Router) {
									substate = 'no-router'

								} else if (!p.Router.activeRequest) {
									substate = 'no-active-request'

								} else if (p.Router.activeRequest !== p.Router.request) {
									substate = 'different-request'
								} else {
									substate = 'unknown';
								}

								wait(checkApp);
							}
							break;

						default:
							substate = 'wait-dom';
							wait(checkDom);
							break;
					}
				} catch (e) {
					error(e);
				}
			}

			// юзер ушел не дождавшись загрузки
			var handleUnload = function (event) {
				radar('unload', true);
			};
			window.addEventListener('beforeunload', handleUnload);

			// грузимся слишком долго
			timeoutTimeout = setTimeout(function() {
				radar('timeout', true);
			}, 30000);

			function clearListeners() {
				window.clearTimeout(waitTimeout);
				window.clearTimeout(timeoutTimeout);
				window.removeEventListener('beforeunload', handleUnload);
			}

			function waitDocument() {
				state = 'wait-document';

				try {
					if (document.readyState !== 'loading') {
						checkApp();

					} else {
						window.addEventListener("DOMContentLoaded", function() {
							checkApp();
						});
					}

				} catch (e) {
					error(e);
				}
			}

			function checkGlobal(name, field, amount) {
				if (!window[field]) {
					radar('no-'+name, true);
					return false;

				} else if (Object.keys(window[field]).length < amount) {
					radar('bad-'+name, true);
					return false;
				}

				return true;
			}

			if (loadLog) {
				if (loadLog.ns) {
					loadLog.wait('loaded', 'html', 'page', checkDom);
				} else {
					loadLog.wait('loaded', 'page', checkDom);
				}

				loadLog.wait('error', function(e) {
					radar('error', true, {
						err: e.error.value
					}, 1);
				});

			} else if (checkGlobal('patron', 'patron', 250) && checkGlobal('ph', '__PH', 20)) {
				waitDocument();
				
			}

		} catch (e) {
			error(e);
		}
	} catch (e) { }
})();
</script>





