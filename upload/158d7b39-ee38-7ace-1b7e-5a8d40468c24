	(function () {
		var isLocked = false, fns = [];
		patron.notificationLocker = function () {
			return {
				isLocked: function () {
					return isLocked;
				},
				setLock: function (status) {
					if (!(isLocked = !!status)) {
						while (fns.length) {
							fns.shift()();
						}
					}
				},
				onUnLock: function (fn) {
					fns.push(fn);
				}
			}
		};
		patron.checkNotificationAccess = function () {
			return true;
		};
	})();

	var FileAPI = {
